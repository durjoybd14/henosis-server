{
  "extends": ["prettier", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "env": {
    "commonjs": true,
    "node": true
  },
  "rules": {
    "arrow-body-style": "off",
    "no-underscore-dangle": "off",
    "new-cap": 0,
    "no-console": 0,
    "indent": 0,
    "linebreak-style": 0,
    "no-unused-vars": "warn",
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "all",
        "singleQuote": true,
        "printWidth": 100,
        "tabWidth": 4,
        "semi": true
      }
    ]
  },
  "plugins": ["prettier"]
}
